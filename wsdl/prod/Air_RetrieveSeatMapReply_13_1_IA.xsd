<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/SMPRES_13_1_IA" xmlns="http://xml.amadeus.com/SMPRES_13_1_IA" elementFormDefault="qualified">
  <xs:element name="Air_RetrieveSeatMapReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">To respond to a request for seat map information for a specific flight and date.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="responseDetails" minOccurs="0" type="ResponseAnalysisDetailsTypeI" />
        <xs:element name="errorInformation" minOccurs="0" type="ErrorInformationTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">Specification of the error details</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="warningInformation" minOccurs="0" type="WarningInformationTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">warning informations</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="seatRequestParameters" minOccurs="0" type="SeatRequestParametersTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">selection criteria for a seat map</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="seatmapInformation" minOccurs="0" maxOccurs="5">
          <xs:annotation>
            <xs:documentation xml:lang="en">Describes each air segment with the seat map of their respective aircrafts.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="flightDateInformation" type="TravelProductInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Travel product identification (flight for which the seat map is requested).</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="seatmapErrorInfo" minOccurs="0" type="ErrorInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Specification of the error details</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="seatmapWarningInfo" minOccurs="0" type="WarningInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">warning informations</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="additionalProductInfo" minOccurs="0" type="AdditionalProductDetailsTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Additional product details</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="equipmentInformation" minOccurs="0" type="EquipmentInfoTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Specification of the details of the aircraft</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="cabin" minOccurs="0" maxOccurs="9">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Seatmap of a cabin.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="compartmentDetails" type="CabinDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">It gives the row where the class starts and where it ends.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="cabinFacilities" minOccurs="0" maxOccurs="9" type="CabinFacilitiesTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Specification of the details of the facilities of  a cabin</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="row" minOccurs="0" maxOccurs="999">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Describes seats characteristics and occupation (One occurence per Row)</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="rowDetails" type="RowDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Row - To specify the individual seat information for seats in a particular row.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="cabinFacility" minOccurs="0" maxOccurs="9" type="CabinFacilitiesTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Specification of the details of the cabin facilities at a certain row.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="freeText" minOccurs="0" maxOccurs="2" type="InteractiveFreeTextTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Pricing and Commercial Description</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="customerCentricData" minOccurs="0" maxOccurs="99">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Price map per passenger or group of passengers</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="dummy" type="DummySegmentTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Separator</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="travellerDetails" minOccurs="0" maxOccurs="99" type="TravellerInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Traveller Details</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="seatPrice" maxOccurs="999">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Prices of chargeables seats for a given passenger</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="seatPrice" type="MonetaryInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Seat Price</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="taxDetails" minOccurs="0" type="TaxTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">tax Details</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="rowDetails" maxOccurs="999" type="RowDetailsTypeI_168302S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Position of the seats by rows, with optional seat occupation at passenger level</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="AdditionalEquipmentInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify equipment configuration, registration number, and cabin facilities.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="fittedConfigurationCode" minOccurs="0" type="AlphaNumericString_Length1To12">
        <xs:annotation>
          <xs:documentation xml:lang="en">Aircraft cabin version identification</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="airlineDetails" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Airline Organisation FOR Airline Owner</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AdditionalProductDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To convey additional information concerning an airline flight.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="legDetails" minOccurs="0" type="AdditionalProductTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Additional product details</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="departureStationInfo" minOccurs="0" type="StationInformationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Departure information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="arrivalStationInfo" minOccurs="0" type="StationInformationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Arrival information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="travellerTimeDetails" minOccurs="0" type="TravellerTimeDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Traveller time details</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="facilitiesInformation" minOccurs="0" maxOccurs="10" type="ProductFacilitiesTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Such things as meals or entertrainment on board</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AdditionalProductTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Additional details describing a specific means of transport.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="equipment" minOccurs="0" type="AlphaNumericString_Length3To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Aircraft code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="numberOfStops" minOccurs="0" type="NumericInteger_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Number of en-route stops</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="duration" minOccurs="0" type="NumericInteger_Length4To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">Leg duration</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="daysOfOperation" minOccurs="0" type="NumericInteger_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Days of the week the flight operates: 1=Monday to 7=Sunday</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinClassDesignationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Product class, number, and designator which define a cabin in a means of transport.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="classDesignator" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Class of the cabin whose seat map is returned.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="bookingClass" minOccurs="0" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Identify the reservation booking designator.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cabinClass" minOccurs="0" type="NumericInteger_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Cabin class of service, coded</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="compartmentDesignator" minOccurs="0" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Cabin compartment designator</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinClassSeatRowRangeDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the range of rows in a particular cabin class.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="number" maxOccurs="2" type="NumericInteger_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Specifies the first and last row number within a particular cabin class compartment.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the physical layout of a cabin area for seating.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="classDetails" minOccurs="0" type="CabinClassDesignationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Cabin class designation</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="seatRowRange" type="CabinClassSeatRowRangeDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">To specify the range of rows in a particular cabin class</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cabinZoneCode" minOccurs="0" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Specifies the physical of a cabin class within a means of transport- lowerdeck-maindeck-upperdeck</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="smokingSeatRowRange" minOccurs="0" type="SmokingAreaSeatRowRangeDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">To specify the range of rows that allows smoking.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="defaultSeatOccupation" minOccurs="0" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Describes the general occupation status of seats on a means of transport</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="overwingSeatRowRange" minOccurs="0" type="OverwingSeatRowRangeTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">To specify the range of rows over the wings of an aircraft</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="columnDetails" minOccurs="0" maxOccurs="12" type="CabinWidthAndColumnDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">List of column designators in this cabin</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinFacilitiesDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Information describing a specific area in a means of transport.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="type" type="AlphaString_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">To indicate the type of cabin facility (e.g. lavatory, galley, etc.).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="location" minOccurs="0" type="AlphaString_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">To indicate the location of a cabin facility within a numbered row (e.g. right, etc.).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinFacilitiesTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To indicate facilities within a cabin class or cabin compartment.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="rowLocation" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Row location qualifier -To indicate the location of a cabin facility . F = front M = middle R = rear.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cabinFacilityDetails" type="CabinFacilitiesDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Information describing a specific area in a means of transport.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="otherCabinFacilityDetails" minOccurs="0" maxOccurs="8" type="CabinFacilitiesDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Information describing a specific area in a means of transport.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinWidthAndColumnDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the status of a column of seats.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="seatColumn" type="AlphaNumericString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">To indicate the column of a seat</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" minOccurs="0" maxOccurs="2" type="AlphaNumericString_Length1To2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Column description - To indicate if the column is along a window or an aisle or between two other columns.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CompanyIdentificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">Code or name to identify a company and any associated companies.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="marketingCompany" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">A 2-3 character code to specify the marketing airline designator code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="operatingCompany" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">A 2-3 character code to specify the operating airline designator code</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ConfigurationDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the number of seats for a specific class of service.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="classDesignator" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Cabin class designator  To indicate cabin class not necessarily an RBD</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="numberOfSeats" minOccurs="0" type="NumericInteger_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Seats - number of seats</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DummySegmentTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To serve the purpose of a mandatory segment at the beginning of a group and to avoid segment collision.</xs:documentation>
    </xs:annotation>
    <xs:sequence />
  </xs:complexType>
  <xs:complexType name="EquipmentInfoTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify configuration details of an aircraft.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="cabinClassDetails" minOccurs="0" maxOccurs="5" type="ConfigurationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">The details of the fitted configuration.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="iataAircraftTypeCode" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">User own or ATA/IATA industry defined aircraft type code (See SSIM, Appendix A, IATA Airline Coding Directory, Chapter 8).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="configVersionDetails" minOccurs="0" type="AdditionalEquipmentInformationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Additional equipment information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="configVersionDescription" minOccurs="0" type="AlphaNumericString_Length1To70">
        <xs:annotation>
          <xs:documentation xml:lang="en">Free text</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ErrorInformationDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify an error and whether it occurred at the system or application level.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="processingLevel" type="NumericInteger_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Indicates at which level of processing the error took place.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="code" minOccurs="0" type="NumericInteger_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">A code assigned by the receiver of a message identifying the error or warning.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" minOccurs="0" type="AlphaNumericString_Length1To70">
        <xs:annotation>
          <xs:documentation xml:lang="en">Free text</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ErrorInformationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the error(s) encountered while processing.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="errorDetails" type="ErrorInformationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Error information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="otherErrorDetails" minOccurs="0" maxOccurs="9" type="ErrorInformationDetailsTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Error information</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FreeTextQualificationTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To specify the type, purpose, and language of free text and whether any action is required.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="textSubjectQualifier" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Text Subject Qualifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="informationType" minOccurs="0" type="AlphaNumericString_Length1To4">
        <xs:annotation>
          <xs:documentation xml:lang="en">Information Type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">status</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="companyId" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">Company Id</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="language" minOccurs="0" type="AlphaNumericString_Length1To3">
        <xs:annotation>
          <xs:documentation xml:lang="en">language</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="GenericDetailsTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To identify the type of seat requested.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="cabinClassDesignator" minOccurs="0" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Allows seat assignment request and response by aircraft cabin.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="noSmokingIndicator" minOccurs="0" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Allows seat assignment request and response by smoking/non-smoking zone.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="cabinClass" minOccurs="0" type="NumericInteger_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Cabin Class of service, coded. Allows seat assignment request/response by cabin class.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="compartmentDesignator" minOccurs="0" type="AlphaString_Length1To1">
        <xs:annotation>
          <xs:documentation xml:lang="en">Allows seat assignment request and response by cabin compartment if there are separate cabins (front, rear cabin or upper deck)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="seatCharacteristic" minOccurs="0" maxOccurs="5" type="AMA_EDICodesetType_Length1to2">
        <xs:annotation>
          <xs:documentation xml:lang="en">Allows seat assignment request/response to provide additional seat detail.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="InteractiveFreeTextTypeI">
    <xs:annotation>
      <xs:documentation xml:lang="en">To provide free text information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="freeTextQualification" minOccurs="0" type="FreeTextQualificationTypeI">
        <xs:annotation>
          <xs:documentation xml:lang="en">Free Text Qualification</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="freeText" minOccurs="0" maxOccurs="99" type="AlphaNumericString_Length1To70">
        <xs:annotation>
          <xs:documentation xml:lang="en">Free text and message sequence numbers of the remarks.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
